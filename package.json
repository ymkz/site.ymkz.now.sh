{
  "name": "ymkz.co",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prepare": "git config --local core.hooksPath .githooks || echo 'CI'",
    "reset": "rm -rf node_modules package-lock.json",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "codecheck": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "lint": "npm run typecheck && npm run codecheck",
    "format": "prettier --write ."
  },
  "lint-staged": {
    "*.{js,ts,tsx,css,yml,json,html,md}": [
      "prettier --write"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "dependencies": {
    "@emotion/react": "^11.4.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "@vitejs/plugin-react-refresh": "^1.3.4",
    "lint-staged": "^11.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.1",
    "prettier-plugin-organize-imports": "^2.2.0",
    "prettier-plugin-packagejson": "^2.2.11",
    "typescript": "^4.3.4",
    "vite": "^2.3.7"
  },
  "engines": {
    "node": "16.x",
    "npm": "7.x"
  },
  "volta": {
    "node": "16.4.0",
    "npm": "7.18.1"
  }
}
