{
  "private": true,
  "name": "ymkz.co",
  "description": "ymkz's website",
  "license": "MIT",
  "author": "ymkz <ymkzly@gmail.com> (https://github.com/ymkz)",
  "homepage": "https://ymkz.co",
  "repository": {
    "type": "git",
    "url": "https://github.com/ymkz/ymkz.co"
  },
  "bugs": {
    "url": "https://github.com/ymkz/ymkz.co/issues"
  },
  "version": "7.0.0",
  "scripts": {
    "build": "run-s build:tcm build:format build:next",
    "build:format": "prettier --write 'src/**/*.module.scss.d.ts'",
    "build:next": "next build",
    "build:tcm": "tcm --pattern src/**/*.module.scss",
    "dev": "run-p dev:tcm dev:format dev:next",
    "dev:format": "onchange 'src/**/*.module.scss.d.ts' -- prettier --write 'src/**/*.module.scss.d.ts'",
    "dev:next": "next dev",
    "dev:tcm": "tcm --watch --pattern src/**/*.module.scss",
    "lint": "run-p lint:*",
    "lint:css": "stylelint 'src/**/*.{js,ts,tsx,css,scss}'",
    "lint:js": "eslint 'src/**/*.{js,ts,tsx}'",
    "start": "next start",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "next": "^9.3.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@secretlint/secretlint-rule-preset-recommend": "^1.0.4",
    "@types/node": "^13.9.8",
    "@types/react": "^16.9.27",
    "@ymkz/eslint-config": "^1.4.0",
    "@ymkz/prettier-config": "^1.1.0",
    "@ymkz/stylelint-config": "^1.1.0",
    "eslint": "^6.8.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.9",
    "npm-run-all": "^4.1.5",
    "onchange": "^6.1.0",
    "prettier": "^2.0.2",
    "prettier-package-json": "^2.1.3",
    "sass": "^1.26.3",
    "secretlint": "^1.0.4",
    "stylelint": "^13.2.1",
    "typed-css-modules": "^0.6.3",
    "typescript": "^3.8.3",
    "yarnhook": "^0.4.3"
  },
  "engines": {
    "node": ">=12"
  }
}
