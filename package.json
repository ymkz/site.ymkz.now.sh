  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@types/node": "14.14.25",
    "@types/react": "17.0.1",
    "husky": "4.3.8",
    "prettier": "2.2.1",
    "prettier-plugin-organize-imports": "1.1.1",
    "prettier-plugin-pkg": "0.8.0",
    "typescript": "4.1.4"
  },
  "commitlint": {
