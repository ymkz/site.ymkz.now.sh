{
  "private": true,
  "license": "MIT",
  "version": "0.0.0",
  "main": "package.json",
  "name": "ymkz.io",
  "homepage": "https://ymkz.io",
  "description": "The portfolio site about ymkz",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "typecheck": "tsc",
    "lint": "eslint --ext .js,.ts,.tsx .",
    "lazy": "run-p typecheck lint build",
    "test:browser": "percy exec -- node tests/index.js",
    "test:integration": "start-test dev http://localhost:3000 test:browser",
    "webapp": "webappgen --file static/images/icons/logo.svg"
  },
  "dependencies": {
    "@egoist/snackbar": "1.2.1",
    "@emotion/core": "10.0.10",
    "@emotion/styled": "10.0.11",
    "next": "8.1.0",
    "next-ga": "2.3.4",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-use-form-state": "0.10.2"
  },
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@commitlint/cli": "7.6.1",
    "@commitlint/config-conventional": "8.0.0",
    "@emotion/babel-preset-css-prop": "10.0.9",
    "@percy/puppeteer": "1.0.8",
    "@types/next": "8.0.5",
    "@types/puppeteer": "1.12.4",
    "@types/react": "16.8.19",
    "@types/react-dom": "16.8.4",
    "@types/throttle-debounce": "1.1.1",
    "@typescript-eslint/eslint-plugin": "1.9.0",
    "@typescript-eslint/parser": "1.9.0",
    "@ymkz/webappgen-cli": "0.1.0",
    "@zeit/next-css": "1.0.1",
    "@zeit/next-typescript": "1.1.1",
    "babel-plugin-module-resolver": "3.2.0",
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.3.0",
    "eslint-import-resolver-babel-module": "5.1.0",
    "eslint-plugin-compat": "3.1.1",
    "eslint-plugin-import": "2.17.3",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.13.0",
    "eslint-plugin-react-hooks": "1.6.0",
    "eslint-plugin-security": "1.4.0",
    "eslint-plugin-unicorn": "8.0.2",
    "husky": "2.3.0",
    "imagemin-mozjpeg": "8.0.0",
    "imagemin-optipng": "7.0.0",
    "imagemin-svgo": "7.0.0",
    "make-dir": "3.0.0",
    "next-compose-plugins": "2.2.0",
    "next-optimized-images": "2.5.1",
    "next-size": "2.0.2",
    "npm-run-all": "4.1.5",
    "prettier": "1.17.1",
    "puppeteer": "1.17.0",
    "responsive-loader": "1.2.0",
    "sharp": "0.22.1",
    "start-server-and-test": "1.9.1",
    "typescript": "3.5.1",
    "webpack": "4.32.2",
    "yarnhook": "0.4.0"
  },
  "author": {
    "name": "ymkz",
    "email": "ymkzly@gmail.com",
    "url": "https://github.com/ymkz"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ymkz/ymkz.io"
  },
  "bugs": {
    "url": "https://github.com/ymkz/ymkz.io/issues"
  },
  "keywords": [
    "serverless",
    "lambda",
    "nextjs",
    "server-side-rendering",
    "react",
    "single-page-application",
    "portfolio"
  ],
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "yarn run lazy",
      "post-checkout": "yarnhook",
      "post-rewrite": "yarnhook",
      "post-merge": "yarnhook"
    }
  }
}
