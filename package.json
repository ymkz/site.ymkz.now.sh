{
  "private": true,
  "license": "MIT",
  "version": "0.0.0",
  "main": "package.json",
  "name": "ymkz.io",
  "homepage": "https://ymkz.io",
  "description": "The portfolio site about ymkz",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "typecheck": "tsc",
    "lint": "eslint --ext .js,.ts,.tsx .",
    "lazy": "run-p typecheck lint build",
    "test:browser": "percy exec -- node tests/index.js",
    "test:integration": "start-test dev http://localhost:3000 test:browser",
    "webapp": "webappgen -i template/icon.svg -t template/manifest.json"
  },
  "dependencies": {
    "@egoist/snackbar": "1.4.0",
    "@emotion/core": "10.0.14",
    "@emotion/styled": "10.0.12",
    "next": "8.1.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-ga": "2.6.0",
    "react-use-form-state": "0.10.4"
  },
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@commitlint/cli": "8.0.0",
    "@commitlint/config-conventional": "8.0.0",
    "@emotion/babel-preset-css-prop": "10.0.14",
    "@percy/puppeteer": "1.0.8",
    "@types/next": "8.0.5",
    "@types/puppeteer": "1.12.4",
    "@types/react": "16.8.22",
    "@types/react-dom": "16.8.4",
    "@typescript-eslint/eslint-plugin": "1.11.0",
    "@typescript-eslint/parser": "1.11.0",
    "@ymkz/webappgen-cli": "1.0.1",
    "@zeit/next-css": "1.0.1",
    "@zeit/next-typescript": "1.1.1",
    "babel-plugin-module-resolver": "3.2.0",
    "eslint": "5.16.0",
    "eslint-config-prettier": "6.0.0",
    "eslint-import-resolver-babel-module": "5.1.0",
    "eslint-plugin-import": "2.18.0",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.14.2",
    "eslint-plugin-react-hooks": "1.6.1",
    "eslint-plugin-security": "1.4.0",
    "eslint-plugin-unicorn": "9.1.1",
    "husky": "2.5.0",
    "imagemin-mozjpeg": "8.0.0",
    "imagemin-optipng": "7.0.0",
    "imagemin-svgo": "7.0.0",
    "make-dir": "3.0.0",
    "next-compose-plugins": "2.2.0",
    "next-offline": "4.0.2",
    "next-optimized-images": "2.5.3",
    "next-size": "2.0.2",
    "npm-run-all": "4.1.5",
    "prettier": "1.18.2",
    "prop-types": "15.7.2",
    "puppeteer": "1.18.1",
    "responsive-loader": "1.2.0",
    "sharp": "0.22.1",
    "start-server-and-test": "1.9.1",
    "typescript": "3.5.2",
    "webpack": "4.35.0",
    "yarnhook": "0.4.1"
  },
  "author": {
    "name": "ymkz",
    "email": "ymkzly@gmail.com",
    "url": "https://github.com/ymkz"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ymkz/ymkz.io"
  },
  "bugs": {
    "url": "https://github.com/ymkz/ymkz.io/issues"
  },
  "keywords": [
    "serverless",
    "lambda",
    "nextjs",
    "server-side-rendering",
    "react",
    "single-page-application",
    "portfolio"
  ],
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "yarn run lazy",
      "post-checkout": "yarnhook",
      "post-rewrite": "yarnhook",
      "post-merge": "yarnhook"
    }
  }
}
