{
  "name": "ymkz.co",
  "version": "7.0.0",
  "private": true,
  "description": "ymkz's website",
  "homepage": "https://ymkz.co",
  "bugs": {
    "url": "https://github.com/ymkz/ymkz.co/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ymkz/ymkz.co"
  },
  "license": "MIT",
  "author": "ymkz <ymkzly@gmail.com> (https://github.com/ymkz)",
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "format": "prettier --write .",
    "lint": "prettier --check .",
    "start": "next start",
    "typecheck": "tsc --noEmit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "post-checkout": "yarnhook",
      "post-merge": "yarnhook",
      "post-rewrite": "yarnhook"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx,css,yml,json,html,md}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "tsc --noEmit"
    ]
  },
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "singleQuote": true
  },
  "dependencies": {
    "dayjs": "1.8.31",
    "next": "9.5.1",
    "next-dark-mode": "2.3.0",
    "next-seo": "4.7.3",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-markdown": "4.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "9.1.2",
    "@commitlint/config-conventional": "9.1.1",
    "@types/node": "14.0.27",
    "@types/react": "16.9.43",
    "husky": "4.2.5",
    "lint-staged": "10.2.11",
    "prettier": "2.0.5",
    "prettier-plugin-packagejson": "2.2.5",
    "typescript": "3.9.7",
    "yarnhook": "0.4.3"
  },
  "engines": {
    "node": "12.x"
  },
  "volta": {
    "node": "12.18.1"
  }
}
