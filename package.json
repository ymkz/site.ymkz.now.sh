{
  "name": "ymkz.co",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prepare": "git config --local core.hooksPath .githooks || echo 'CI'",
    "reset": "rm -rf node_modules package-lock.json",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "codecheck": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "lint": "npm run typecheck && npm run codecheck",
    "format": "prettier --write ."
  },
  "lint-staged": {
    "*.{js,ts,tsx,css,yml,json,html,md}": [
      "prettier --write"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "dependencies": {
    "solid-js": "^1.0.0",
    "solid-styled-components": "^0.27.1"
  },
  "devDependencies": {
    "@types/babel__core": "^7.1.14",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.1",
    "prettier-plugin-organize-imports": "^2.2.0",
    "prettier-plugin-packagejson": "^2.2.11",
    "typescript": "^4.3.4",
    "vite": "^2.3.7",
    "vite-plugin-solid": "^2.0.0",
    "vite-tsconfig-paths": "^3.3.13"
  },
  "engines": {
    "node": "16.x",
    "npm": "7.x"
  },
  "volta": {
    "node": "16.4.0",
    "npm": "7.18.1"
  }
}
