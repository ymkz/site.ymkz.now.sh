{
  "name": "ymkz.co",
  "private": true,
  "scripts": {
    "prepare": "git config --local core.hooksPath .githook || echo 'CI'",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "codecheck": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "lint": "npm run typecheck && npm run codecheck",
    "format": "prettier --write ."
  },
  "lint-staged": {
    "*.{js,ts,tsx,css,yml,json,html,md}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "tsc --noEmit"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "dependencies": {
    "next": "10.1.3",
    "next-seo": "4.24.0",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@types/node": "14.14.37",
    "@types/react": "17.0.3",
    "lint-staged": "10.5.4",
    "prettier": "2.2.1",
    "prettier-plugin-organize-imports": "1.1.1",
    "prettier-plugin-packagejson": "2.2.10",
    "prettier-plugin-style-order": "0.2.2",
    "typescript": "4.2.3",
    "typescript-plugin-css-modules": "3.2.0"
  },
  "engines": {
    "node": "14.x",
    "npm": "7.x"
  },
  "volta": {
    "node": "14.16.0",
    "npm": "7.6.2"
  }
}
