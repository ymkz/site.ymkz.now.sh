{
  "name": "ymkz.co",
  "version": "7.0.0",
  "private": true,
  "description": "ymkz's website",
  "homepage": "https://ymkz.co",
  "bugs": {
    "url": "https://github.com/ymkz/ymkz.co/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ymkz/ymkz.co"
  },
  "license": "MIT",
  "author": "ymkz <ymkzly@gmail.com> (https://github.com/ymkz)",
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "lint": "run-p lint:*",
    "lint:css": "stylelint 'src/**/*.{ts,tsx,css}'",
    "lint:js": "eslint 'src/**/*.{ts,tsx}'",
    "start": "next start",
    "typecheck": "tsc --noEmit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "post-checkout": "yarnhook",
      "post-merge": "yarnhook",
      "post-rewrite": "yarnhook"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{md,yml,json,html}": [
      "prettier --write"
    ],
    "*.{ts,tsx,css,scss}": [
      "stylelint --fix"
    ],
    "*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "prettier": "@ymkz/prettier-config",
  "eslintConfig": {
    "extends": [
      "@ymkz/eslint-config",
      "@ymkz/eslint-config/react",
      "@ymkz/eslint-config/import",
      "@ymkz/eslint-config/typescript"
    ],
    "root": true
  },
  "stylelint": {
    "extends": "@ymkz/stylelint-config"
  },
  "dependencies": {
    "next": "9.4.4",
    "next-seo": "4.5.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-markdown": "4.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@types/node": "14.0.11",
    "@types/react": "16.9.35",
    "@types/webpack-env": "1.15.2",
    "@ymkz/eslint-config": "1.6.0",
    "@ymkz/prettier-config": "1.2.0",
    "@ymkz/stylelint-config": "1.1.0",
    "eslint": "7.2.0",
    "front-matter": "4.0.2",
    "husky": "4.2.5",
    "lint-staged": "10.2.9",
    "npm-run-all": "4.1.5",
    "prettier": "2.0.5",
    "prettier-plugin-packagejson": "2.2.5",
    "raw-loader": "4.0.1",
    "sass": "1.26.8",
    "stylelint": "13.6.0",
    "typescript": "3.9.5",
    "yarnhook": "0.4.3"
  },
  "engines": {
    "node": "12.x"
  },
  "volta": {
    "node": "12.18.0"
  }
}
