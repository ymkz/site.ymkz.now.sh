  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@types/node": "14.14.6",
    "@types/react": "16.9.55",
    "husky": "4.3.0",
    "next-secure-headers": "2.0.0",
    "prettier": "2.1.2",
    "prettier-plugin-organize-imports": "1.1.1",
    "prettier-plugin-pkg": "0.8.0",
